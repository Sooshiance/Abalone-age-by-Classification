{
    "manifest_version": 3,
    "name": "SHA-2 Hasher",
    "version": "1.0",
    "description": "Hashes a value using SHA-2 algorithm",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/images.png"
        }
    }
}